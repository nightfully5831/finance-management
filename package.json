{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend server of Biddi Cars",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod",
    "watch:dev": "nodemon",
    "build": "npm run clean && npm run transpile",
    "server": "node -r dotenv/config ./dist/server.js",
    "dev": "npx cross-env NODE_ENV=development npm-run-all build post-build server",
    "preinstall": "npx cross-env ACCEPT_HIGHCHARTS_LICENSE=1",
    "prod": "npx cross-env NODE_ENV=production && npm run build && npm run post-build && npm run server",
    "oldProd": "npx cross-env NODE_ENV=production npm-run-all build post-build server",
    "post-build": "cpx \"public/*\" \"dist/public\" && cpx \"templates/*\" \"dist/templates\" && cpx \"./swagger.json\" \"dist/\"",
    "heroku-cleanup": "cpx \"public/*\" \"dist/public\" && cpx \"templates/*\" \"dist/templates\" && cpx \"./swagger.json\" \"dist/\"",
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "squeaky-clean": "rimraf dist node_modules",
    "lint": "eslint \"./**/*.js\" --quiet",
    "lint:es": "eslint --ext .js,.jsx .",
    "lintFull": "eslint \"./**/*.js\"",
    "lintFix": "eslint --fix --ext .js,.jsx ."
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "public/*",
      "templates/*",
      "swagger.yaml",
      "swagger.json"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "keywords": [],
  "author": "Dojo Design & Developments",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-security": "^1.5.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.17.2",
    "aws-sdk": "^2.1087.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "cognito-express": "^3.0.1",
    "convert-excel-to-json": "^1.7.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.6",
    "express": "^4.21.0",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.0",
    "express-sse": "^0.5.3",
    "firebase-admin": "^12.3.0",
    "generate-password": "^1.7.0",
    "google-auth-library": "^9.14.1",
    "html-to-text": "^8.1.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "merge-pdf-buffers": "^1.0.3",
    "mime-types": "^2.1.35",
    "mongoose": "^6.2.1",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
    "mongoose-paginate-v2": "^1.6.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "multer-storage-cloudinary": "^4.0.0",
    "node-base64-image": "^2.0.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.1",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "ordersid-generator": "^1.9.3",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "puppeteer": "^14.1.2",
    "quickchart-js": "^3.0.1",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "stacktracey": "^2.1.8",
    "stripe": "^8.222.0",
    "swagger-ui-express": "^4.5.0",
    "twilio": "^4.22.0",
    "yamljs": "^0.3.0"
  }
}
